{
  "name": "react-webpack-starter",
  "version": "0.0.1",
  "description": "React boilerplate with Webpack (dev and production) configuration",
  "scripts": {
    "build-dev": "./node_modules/.bin/webpack-dev-server --config webpack.dev.config.js --progress",
    "build-production": "NODE_ENV=production ./node_modules/.bin/webpack --config webpack.production.config.js"
  },
  "keywords": ["react","webpack","boilerplate"],
  "author": "MS",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
